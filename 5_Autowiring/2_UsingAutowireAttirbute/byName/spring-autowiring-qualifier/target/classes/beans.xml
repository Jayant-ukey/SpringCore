<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
			"https://www.springframework.org/dtd/spring-beans-2.0.dtd">
			
<beans>
	<bean id="heart" class="com.seleniumexpress.autowired.Heart" ></bean>
	
	<!-- Here we are performing autowire byName. So reference variable name and object name(bean id) that you specified in the bean tag must be same. -->
	<bean id="humanObj" class="com.seleniumexpress.autowired.Human" autowire="byName">
	
	</bean>
</beans>
